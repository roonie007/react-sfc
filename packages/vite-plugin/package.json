{
  "name": "vite-plugin-react-sfc",
  "description": "Vite plugin for React SFC",
  "module": "dist/index.js",
  "type": "module",
  "version": "0.0.3",
  "author": {
    "name": "Mouadh HSOUMI",
    "email": "hsmouadh@gmail.com",
    "url": "https://github.com/roonie007"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roonie007/react-sfc.git"
  },
  "bugs": {
    "url": "https://github.com/roonie007/react-sfc/issues"
  },
  "scripts": {
    "compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
    "build:clean": "rm -rf ./dist",
    "build": "bun run build:clean compile && bun run compile"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "ast-types": "^0.14.2",
    "case-anything": "^3.1.0",
    "recast": "^0.23.9",
    "vite": "6"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vite",
    "plugin",
    "react",
    "sfc",
    "single",
    "file",
    "component",
    "typescript",
    "jsx",
    "tsx"
  ]
}
